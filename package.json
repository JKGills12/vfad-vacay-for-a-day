    "url": "https://github.com/JKGills12/vfad2/issues"
  },
  "homepage": "https://github.com/JKGills12/vfad2#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "mongoose": "^5.7.8",
    "mustache-express": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  }
}
